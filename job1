pipeline {
     agent any
     options { timestamps ()}

    stages {
        stage('Build') {
            steps {
                echo 'Build code'
            }
        }
        stage('Test') {
            steps {
                echo 'test code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy code'
            }
        }
     stage ('Build JobName')
     {
        steps {
                
                echo "Job Name is ${JOB_NAME}"
               }
      }
     stage ('Build No.')
     {
        steps {
                
                echo "Build number is ${currentBuild.number}"
               }
      }
     stage ('Build JobUrl')
     {
        steps {
                
                echo "Job URL is ${JOB_URL}"
               }
      }
    
      stage('Build Date') {
        steps {
            script {
                    def today = new Date()
                    println "Today: " + today.format("MM/dd/yyyy")
                } 
            }
        }
     stage('python') {
       steps{
            bat 'C:\\Users\\lavpa\\AppData\\Local\\Programs\\Python\\Python310\\python.exe build.py'
             }
         }    
  }
}
 post 
        {
           always {
               printFinishedStageDurations()
        }
    }
