pipeline {
     agent any
     stages {

     stage ('Build JobName')
     {
        steps {
                
                echo "Job Name is ${JOB_NAME}"
               }
      }
     stage ('Build No.')
     {
        steps {
                
                echo "Build number is ${currentBuild.number}"
               }
      }
     stage ('Build JobUrl')
     {
        steps {
                
                echo "Job URL is ${JOB_URL}"
               }
      }
    stage ('Build JobTS')
     {
        steps {
                
                echo "TimeStamp: ${currentBuild.startTimeInMillis}"
               }
      }
    stage ('Build JobDuration')
     {
        steps {
                
               echo "Time Duration= ${currentBuild.durationString.minus(' and counting')}"
              }
      }
      stage('Build Date') {
        steps {
            script {
                    def today = new Date()
                    println "Today: " + today.format("MM/dd/yyyy")
                } 
            }
        }
     stage(python)
       steps{
             bat 'arg.py'
             }
  }
}
