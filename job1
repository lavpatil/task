pipeline {
     agent any
     stages {

     stage ('Build JobName')
     {
        steps {
                
                echo "Job Name is ${JOB_NAME}"
               }
      }
     stage ('Build No.')
     {
        steps {
                
                echo "Build number is ${currentBuild.number}"
               }
      }
     stage ('Build JobUrl')
     {
        steps {
                
                echo "Job Name is ${JOB_URL}"
               }
      }
  }
}
