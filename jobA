pipeline {
    agent any
    options { timestamps ()}

    stages {
        stage('Build') {
            steps {
                echo 'Build code'
            }
        }
        stage('Test') {
            steps {
                echo 'test code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy code'
            }
        }
        stage('End') {
            steps {
                    echo "end= ${previousBuild.timestamps ()}"
                  }
            }      
     }
}
