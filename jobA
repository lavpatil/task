pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Build code'
            }
        }
        stage('Test') {
            steps {
                echo 'test code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy code'
            }
        }
        stage ('Build job1'){
            steps {
                build job: 'job1'
            }
        } 
    }
}
