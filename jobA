pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Build code'
            }
        }
        stage('Test') {
            steps {
                echo 'test code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy code'
            }
        }
        stage ('Build JobB'){
        steps {
            build job: 'jobB.py', parameters: [string(name: "Environment", value: "production")]
        }
    }
    }
}
