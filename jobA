pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo '${(currentBuild.startTimeInMillis)/1000} Build code'
            }
        }
        stage('Test') {
            steps {
                echo '${(currentBuild.startTimeInMillis)/1000} test code'
            }
        }
        stage('Deploy') {
            steps {
                echo '${(currentBuild.startTimeInMillis)/1000} Deploy code'
            }
        }
    }
}
