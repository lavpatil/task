pipeline {
    agent any
    options { timestamps ()}

    stages {
        stage('Build') {
            steps {
                echo 'Build code'
            }
        }
        stage('Test') {
            steps {
                echo 'test code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy code'
            }
        }
        stage('Get time of previous build') {
            steps {
                    echo "currentBuild.rawBuild.getPreviousBuild().getTimestamp()"
                   }    
            }
      }
}
