pipeline {
    agent any
    options{[Timestamp]}

    stages {
        stage('Build') {
            steps {
                echo 'Build code'
            }
        }
        stage('Test') {
            steps {
                echo "${(currentBuild.startTimeInMillis)/1000} test code"
            }
        }
        stage('Deploy') {
            steps {
                echo "${(currentBuild.startTimeInMillis)/1000} Deploy code"
            }
        }
    }
}
