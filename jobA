pipeline {
    agent any
    options { timestamps ()}

    stages {
        stage('Build') {
            steps {
                echo 'Build code'
            }
        }
        stage('Test') {
            steps {
                echo 'test code'
            }
        }
        stage('Deploy') {
            steps {
                echo "Deploy code ${currentBuild.durationString.minus(' and counting')}"
            }
        }
        stage('Build Number') {
            steps {
                echo "Build Number is  ${currentBuild.number},Job Duration ${currentBuild.durationString.minus(' and counting')}"
            }
        }
        stage('python') {
            steps {
                   bat 'C:\\Users\\lavpa\\AppData\\Local\\Programs\\Python\\Python310\\python.exe time.py'
           }
        }
     }
}
